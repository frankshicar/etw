<?xml version="1.0" encoding="utf-8"?>
<Configuration>
	<FileSystemWatcherConfig>
		<Path>C:\Users\User\Desktop</Path>
		<Filter>*.*</Filter>
		<NotifyFilter>FileName,DirectoryName,LastWrite</NotifyFilter>
	</FileSystemWatcherConfig>
	<ProcessMonitorConfig>
		<Name>chrome.exe</Name>
		<Name>msedge.exe</Name>
	</ProcessMonitorConfig>
</Configuration>

